<?xml version="1.0"?>
<?xml-stylesheet type="text/xsl" href="configuration.xsl"?>
 
<configuration>


 <property>
    <name>content</name>
    <description>This is the jinja template for /etc/sysconfig/vncservers file</description>
    <value>
#users
VNCSERVERS="1:{{vnc_user}}"

